from random import randint

p = raw_input('Please enter the prime: ')
def cr(p):
  if i % 3 == 1:
      v = (i - 1) / 3
      t = randint(2, i - 1)
      while (pow(t, v, i) == 1):
          t = randint(2, i - 1)
      w = pow(t, v, i)
      return [w + 1, i + 1 - (w + 1), i - 1]
  else:
      return [i - 1]
